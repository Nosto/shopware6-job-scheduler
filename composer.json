{
    "name": "nosto/shopware6-job-scheduler",
    "description": "System library for async job scheduling for Shopware 6",
    "type": "library",
    "version": "2.0.7",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Nosto\\Scheduler\\": "src/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "shopware/administration": "^6.5",
        "symplify/easy-coding-standard": "^12.0"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "scripts": {
        "lint": "./vendor/bin/ecs check",
        "fix": "./vendor/bin/ecs check --fix"
    }
}
